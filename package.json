{
  "name": "PasswordEpic",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "start-clear": "react-native start --reset-cache",
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage --verbose",
    "security-audit": "npm audit --audit-level=moderate",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "format-check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "clear-app": "adb shell pm clear com.passwordepic.mobile",
    "update:firebase:debug": "node update-google-services-debug.js",
    "update:firebase:release": "node update-google-services-release.js",
    "android:debug": "npm run update:firebase:debug && react-native run-android",
    "android:release": "npm run update:firebase:release",
    "android:build-apk": "npm run update:firebase:release && cd android && ./gradlew assembleRelease",
    "android:build-aab": "npm run update:firebase:release && cd android && ./gradlew bundleRelease"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.24.0",
    "@react-native-clipboard/clipboard": "^1.16.3",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-google-signin/google-signin": "^16.0.0",
    "@react-native/new-app-screen": "0.81.4",
    "@react-navigation/bottom-tabs": "^7.4.7",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@reduxjs/toolkit": "^2.9.0",
    "@types/crypto-js": "^4.2.2",
    "@types/react-native-vector-icons": "^6.4.18",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "firebase": "^12.3.0",
    "jail-monkey": "^2.8.4",
    "pako": "^2.1.0",
    "react": "19.1.0",
    "react-native": "0.81.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-device-info": "^14.1.1",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-get-random-values": "^1.11.0",
    "react-native-image-picker": "^8.2.1",
    "react-native-keychain": "^10.0.0",
    "react-native-permissions": "^5.4.2",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-video": "^6.0.0",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.4",
    "@react-native/eslint-config": "0.81.4",
    "@react-native/metro-config": "0.81.4",
    "@react-native/typescript-config": "^0.81.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.6.1",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.1.0",
    "@types/react-native": "^0.72.8",
    "@types/react-redux": "^7.1.34",
    "@types/react-test-renderer": "^19.1.0",
    "@types/yargs": "^17.0.33",
    "audit-ci": "^7.1.0",
    "eslint": "^8.19.0",
    "is-arrayish": "^0.3.4",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20"
  }
}
